{
  "importOrder": [
    "^@kubernetes?.+$",
    "^electron?.+$",
    "^react?.+$",
    "^antd?.+$",
    "^@ant-design?.+$",
    "^@tanstack?.+$",
    "^@reduxjs?.+$",
    "^@rjsf?.+$",
    "^(?![@\\.]).+$",
    "^@constants",
    "^@models",
    "^@redux",
    "^@pages",
    "^@components",
    "^@hooks",
    "^@swaggerUI",
    "^@utils",
    "^@assets",
    "^@styles",
    "^@src",
    "^(?!\\.+\\/styled)^[./]?.+$",
    "^\\.+\\/styled$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false
}
